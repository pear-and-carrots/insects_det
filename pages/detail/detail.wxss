page {
  background-color: #F0F8E4;
}

.nBox {
  display: flex;
  flex-direction: row;
  position: relative;
  font-size: 28rpx;
  font-weight: 600;
  padding-left: 40rpx;
  padding-right: 30rpx;
  justify-content: space-between;
}

.nBox image {
  width: 40rpx;
  height: 40rpx;
  margin: 10rpx;
}

.return {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 40rpx;
  text-align: center;
  background-color: rgb(142, 205, 101);
  border: 4rpx solid rgb(91, 189, 27);
  color: white;
  padding: 10rpx;
  margin-top: 15rpx;
  margin-right: 15rpx;
  margin-bottom: 15rpx;
  border-radius: 10rpx;
}

.message {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 40rpx;
  text-align: center;
  background-color: rgb(236, 90, 82);
  border: 4rpx solid rgb(179, 22, 8);
  color: white;
  padding: 10rpx;
  margin: 15rpx;
  border-radius: 10rpx;
}

.listBox {
  width: 92%;
  margin: 4%;
  position: relative;
}

.repair {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: flex-end;
  margin-top: -50rpx;
  text-align: end;
}

.repair text {
  font-size: 30rpx;
  color: #666666;
  background-color: rgb(233, 250, 233);
  box-shadow: 1rpx 1rpx 1rpx 1rpx #5f8b0c;
  border-radius: 10rpx;
  padding: 8rpx;
}

.swiper {
  width: 100%;
  height: 400rpx;
  border-radius: 20rpx;
  position: relative;
}

.swImg {
  width: 100%;
  border-radius: 20rpx;
}

.listDetail {
  padding: 30rpx;
  margin-top: 20rpx;
  background-color: #fff;
  border-radius: 20rpx;
  box-shadow: 2rpx 2rpx 2rpx 2rpx #e4dac0;
  font-size: 28rpx;
}

.ld {
  padding: 10rpx;
  margin-top: 10rpx;
  border-bottom: 1rpx dotted #e4dac0;
}

input {
  font-size: 28rpx;
  font-weight: 500;
  padding: 10rpx;
  background-color: #eeece6;
  border-radius: 10rpx;
}

.button {
  color: #fff;
  margin-top: 40rpx;
  background-color: #5f8b0c;
  border: 10rpx solid #e9fae0;
  margin-left: 40%;
}

.nullImg {
  text-align: center;
  margin: 40rpx;
  font-size: 32rpx;
  font-weight: 600;
  color: #666666;
}

/* 修改图片 */

.repairImgText {
  z-index: 1;
  position: absolute;
  margin-top: 15rpx;
  margin-left: 72%;
  padding: 10rpx;
  background-color: #fff;
  border-radius: 10rpx;
  opacity: 0.9;
  font-weight: 600;
  font-size: 28rpx;
}

.nullimgBox {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.setImgText {
  width: 150rpx;
  text-align: center;
  padding: 10rpx;
  background-color: rgb(125, 161, 82);
  color: #fff;
  border-radius: 10rpx;
  opacity: 0.9;
  font-weight: 600;
  font-size: 28rpx;
  margin-bottom: 20rpx;
}

.repairImgBox {
  margin: 4%;
  border: 4rpx dashed #5f8b0c;
  text-align: center;
  padding: 30rpx;
  border-radius: 20rpx;
}

.repairImgBox text{
  font-size: 42rpx;
  font-weight: 660;
  color: #365006;
}

.img-box {
  border-bottom: 1px solid #000;
  color: #ccc;
  font-size: 320rpx;
  justify-content: center;
  align-items: center;
  display: flex;
  border: 5rpx;
  border-color: rgba(0, 0, 0, 0.452);
  width: 250rpx;
  height: 250rpx;
  margin-left: 32rpx;
  margin-right: 20rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  position: relative;
  background-color: #fff;
  padding: 10rpx;
  border-radius: 10rpx;
  align-items: center;
  justify-content: center;
}

/* 删除图片 */
.img-delect {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  position: absolute;
  right: -20rpx;
  top: -20rpx;
}

.img {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.inputBox {
  padding: 10rpx;
  margin-bottom: 20rpx;
  background-color: #F8FCF1;
  border: 1rpx solid #BCE085;
  border-radius: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.input {
  width: 67%;
  background-color: #fff;
  border: 1rpx solid rgb(231, 227, 227);
  border-radius: 10rpx;
  padding: 10rpx;
}

.close {
  background-color: #5f8b0c;
  color: #fff;
  border-radius: 100%;
  padding: 8rpx;
  font-size: 32rpx;
  font-weight: 600;
  text-align: center;
  line-height: 55rpx;
  height: 50rpx;
  width: 50rpx;
  margin-left: 88%;
  margin-top: -60rpx;
  margin-bottom: 20rpx;
}

.images_box {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.images_box button {
  color: #fcfcfc;
  background-color: #365006;
}

.imgBox{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.btnText{
  color: #fff;
  background-color: rgb(236, 90, 82);
  padding: 10rpx;
  border-radius: 50rpx;
  font-weight: 600;
  font-size: 25rpx;
}

.addbtnText{
  color: #fff;
  margin-bottom: 50rpx;
  padding: 10rpx;
  border-radius: 50rpx;
  font-weight: 600;
  font-size: 25rpx;
  background-color: rgb(142, 205, 101);
}